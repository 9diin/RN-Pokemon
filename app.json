{
    "expo": {
        "name": "pokemon",
        "slug": "pokemon",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "",
        "scheme": "pokemon",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "",
                "backgroundColor": "#ffffff"
            },
            "edgeToEdgeEnabled": true
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": ""
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#ffffff"
                }
            ],
            "expo-font"
        ],
        "experiments": {
            "typedRoutes": true
        },
        "assets": ["./assets/fonts"]
    }
}
